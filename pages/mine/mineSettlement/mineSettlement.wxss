page{
  background:#f9f9f9;
}
.blue{
  color:#0075eb;
}
.gray{
  color:#666;
}
.clearfix:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
}
.settle-tab{
  background: #fff;
}

.settle-tab .tab-item{
  height:110rpx;
  line-height:110rpx;
  width:calc(100% / 2);
  float:left;
  text-align:center;
  font-family: PingFang-SC-Bold;
	font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
  position:relative;
}
.settle-tab .tab-item .item-left{
  position:absolute;
  background-color: #e4e4e4;
	border-radius: 1rpx;
  width: 2rpx;
	height: 32rpx;
  top: 40rpx;
  left:0;
}

.settle-tab .tab-item .item-right{
  background-color: #e4e4e4;
	border-radius: 1rpx;
  width: 2rpx;
	height: 32rpx;
  position:absolute;
  top: 40rpx;
  right:0;
}

